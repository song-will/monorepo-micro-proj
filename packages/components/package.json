{
  "name": "@exile_song/vue-components",
  "version": "0.0.1",
  "description": "组件库",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/xt-vue-components.common.js",
  "scripts": {
    "init": "npm install --legacy-peer-deps --ignore-scripts && npm rebuild node-sass",
    "dev": "npm run docs:dev",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "fat": "vuepress build docs",
    "clean": "rimraf lib",
    "build": "npm run clean && npm run build:entry && npm run build:rollup && npm run build:components && npm run build:style",
    "build:entry": "node build/bin/build-entry.js",
    "build:style": "node build/bin/build-style.js",
    "build:common": "webpack --config build/webpack.common.js",
    "build:rollup": "rollup --config build/rollup.common.js",
    "build:components": "webpack --config build/webpack.component.js",
    "dev:play": "npm run build && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.play.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "snapshot:prepare_test": "node ./ci_cd/copySnapshots.js --SNAPSHOT_OP=prepare_test",
    "snapshot:update_repo": "node ./ci_cd/copySnapshots.js --SNAPSHOT_OP=update_repo",
    "test:unit": "cross-env TEST_TYPE=unit jest --config src/test/jest.conf.js --coverage",
    "test:snapshot_markup": "cross-env TEST_TYPE=snapshot_markup jest --config src/test/jest.conf.js",
    "test:snapshot_visual": "cross-env TEST_TYPE=snapshot_visual jest --config src/test/jest.conf.js",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag",
    "jenkinsPublish": "npm publish"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "axios": "^0.19.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "date-fns": "^2.6.0",
    "element-ui": "^2.12.1",
    "iview": "3.4.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "querystring": "^0.2.0",
    "raf": "^3.4.1",
    "shortid": "^2.2.15",
    "vue-easytable": "^2.17.2",
    "vuedraggable": "^2.23.2"
  },
